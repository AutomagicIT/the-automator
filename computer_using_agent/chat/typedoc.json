{
  "entryPoints": ["./src"],
  "out": "../../docs/_build/typedoc",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "theme": "markdown",
  "hideGenerator": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace", 
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Function",
    "Accessor",
    "Variable"
  ]
}